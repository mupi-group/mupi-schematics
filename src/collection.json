  "schematics": {
    "application": {
      "factory": "./lib/application/application.factory#main",
      "description": "Create a Mupi application.",
      "schema": "./lib/application/schema.json"
    },
    "model": {
      "factory": "./lib/model/model.factory#main",
      "description": "Create a Mupi model.",
      "schema": "./lib/model/schema.json"
    },
    "env": {
      "factory": "./lib/model/environment.factory#main",
      "description": "Create a Mupi env.",
      "schema": "./lib/env/schema.json"
    },
    "graphql-schema": {
      "factory": "./lib/graphql-schema/graphql-schema.factory#main",
      "description": "Generate a Mupi env.",
      "schema": "./lib/graphql-schema/schema.json"
    },
    "express-service": {
      "factory": "./lib/express-service/express-service.factory#main",
      "description": "Generate a Mupi env.",
      "schema": "./lib/express-service/schema.json"
    }
  }
